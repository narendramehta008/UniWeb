<UserControl x:Class="UniWeb.View.TabContentView.AccountsTabContent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UniWeb.View.TabContentView"
             mc:Ignorable="d" 
             xmlns:dragablz="http://dragablz.net/winfx/xaml/dragablz"
             xmlns:accountTab="clr-namespace:UniWeb.View.Account"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid >
        <dragablz:TabablzControl>
            <TabItem Header="{DynamicResource LangAccount}">
                <accountTab:AccountsView/>
            </TabItem>
            <!--<TabItem Header="{DynamicResource LangFetchData}">
                <ContentControl Content="{Binding FetchDataTab.Value}"></ContentControl>
            </TabItem>-->
        </dragablz:TabablzControl>  
    </Grid>
</UserControl>
